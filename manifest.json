{
  "manifest_version": 3,
  "name": "LinkedIn Ad Blocker",
  "version": "1.0.0",
  "description": "Automatically removes promoted content from LinkedIn feeds",
  
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  
  "content_scripts": [
    {
      "matches": ["https://www.linkedin.com/*"],
      "js": ["content. js"],
      "run_at": "document_idle"
    }
  ],
  
  "permissions": []
}